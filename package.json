{
	"name": "calltree",
	"publisher": "daniel",
	"displayName": "CallTree",
	"description": "",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:calltree.build", "onCommand:calltree.find", "onCommand:calltree.goto"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "calltree.build",
				"title": "CallTree: Build"
			},
			{
				"command": "calltree.find",
				"title": "CallTree: Find"
			},
			{
				"command": "calltree.goto",
				"title": "CallTree: Goto"
			}
		],
		
		
		"views": {
			"explorer": [
				{
					"id": "nodeDependencies",
					"name": "CallTree",
					"contextualTitle": "what ever",
					"visibility": "visible"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
